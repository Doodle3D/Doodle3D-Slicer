{
  "env": {
    "module": {
      "presets": [
        ["env", {
          "targets": {
            "node": "6",
            "browsers": ["last 2 versions", "safari >= 7", "not ie < 11"]
          },
          "modules": false
        }],
        "react"
      ]
    },
    "main": {
      "presets": ["env", "react"]
    }
  },
  "plugins": [
    "babel-plugin-transform-object-rest-spread",
    "babel-plugin-transform-class-properties",
    "babel-plugin-transform-runtime",
    "babel-plugin-transform-es2015-classes"
  ]
}
